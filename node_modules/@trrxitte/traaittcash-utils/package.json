{
  "name": "@trrxitte/traaittcash-utils",
  "version": "0.0.1",
  "description": "A Node.js collection of utilities useful for traaittCASH Developers",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "config.json",
    "index.d.ts",
    "dist/traaittCASHUtils.js",
    "lib/*"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "standard && mocha tests/tests.js",
    "build-docs": "jsdoc -c jsdoc.json",
    "prepare": "webpack"
  },
  "keywords": [
    "traaittCASH",
    "Utilities"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trrxitte/traaittcash-utils.git"
  },
  "author": "TRRXITTE Int., incorporate",
  "contributors": [],
  "license": "GPL-3.0",
  "optionalDependencies": {
    "@trrxitte/traaittcash-crypto": "^0.0.1"
  },
  "dependencies": {
    "crc": "^3.8.0",
    "get-random-values": "^1.2.0",
    "numeral": "^2.0.6",
    "secure-random-string": "^1.1.0",
    "varint": "^5.0.0"
  },
  "standard": {
    "ignore": [
      "lib/traaittcash-crypto/traaittcash-crypto.js",
      "lib/traaittcash-crypto/traaittcash-crypto-wasm.js",
      "dist/*",
      "docs/*"
    ]
  },
  "devDependencies": {
    "mocha": "^6.2.2",
    "standard": "^12.0.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "bugs": {
    "url": "https://github.com/trrxitte/traaittcash-utils/issues"
  },
  "homepage": "https://github.com/trrxitte/traaittcash-utils#readme",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "tests"
  }
}
